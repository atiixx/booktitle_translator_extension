{
  "name": "booktitle-translator",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/polyfill": "7.2.5",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "del": "^2.2.0",
    "main-bower-files": "^2.11.1",
    "run-sequence": "^1.1.5",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "axios": "^1.3.2"
  }
}
